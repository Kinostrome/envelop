{
  "name": "@envelop/graphql-executor",
  "version": "0.0.0",
  "description": "A customizable GraphQL Spec compliant Executor",
  "license": "MIT",
  "author": "Saihajpreet Singh <saihajpreet.singh@gmail.com> (https://saihaj.dev/)",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/envelop.git",
    "directory": "packages/plugins/graphql-executor"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "scripts": {
    "test": "jest",
    "prepack": "bob prepack"
  },
  "dependencies": {
    "graphql-executor": "^0.0.11"
  },
  "devDependencies": {
    "bob-the-bundler": "1.5.1",
    "graphql": "16.0.1",
    "typescript": "4.4.4"
  },
  "peerDependencies": {
    "@envelop/core": "^1.6.1",
    "graphql": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  }
}
